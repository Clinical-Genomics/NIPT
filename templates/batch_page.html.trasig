<script>
    jQuery(function () {
    jQuery('#myTab a:first').tab('show')
    })
</script>

<script>
    $(document).ready(function() {
    var table = $('#batch_table').DataTable( {
        lengthChange: false,
        buttons:    [ 'excel', 'pdf' ],
        paging:     false,
        info: false,
        searching:  false
    } );
    table.buttons().container()
        .appendTo( '#batch_table_wrapper .col-sm-6:eq(0)' );
} );
</script>



</body>
</html>



